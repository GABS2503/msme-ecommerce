{
  "kind": "collectionType",
  "collectionName": "shops",
  "info": {
    "singularName": "shop",
    "pluralName": "shops",
    "displayName": "Shop"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "seller": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "shop"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product",
      "mappedBy": "shop"
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service",
      "mappedBy": "shop"
    },
    "livestream": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::livestream.livestream",
      "mappedBy": "shop"
    }
  }
}
